def function(a,b):
    print(a,b)
    total=len(a)+len(b)
    median=total/2
    x=0
    y=0
    while(median>=0):
        print median
        print (x,y)
        if x<len(a):
            if a[x]>b[y] :
                y=y+1
            else:
                x=x+1

        else:

            y=y+1
        median = median - 1


    if(x<len(a)):
        return a[x+median]
    else:
        return b[y+median]




a=[2]
b=[2,4,7,3,8,1,0,10]
print(function(sorted(a),sorted(b)))